    <script type="text/javascript">
        function afinit() {
            document.getElementById('#search').style.width  = '506px';
            document.getElementById('#navibar').style.width = '797px';
            document.getElementById('#delta2').style.width = '600px';
            document.getElementById('#delta2').style.marginLeft = '30px';
            document.getElementById('#gamma1').style.marginLeft = '-10px';
            document.getElementById('#ul1').style.marginLeft = '-30px';
            document.getElementById('#delta').style.display = 'none';
//            document.getElementById('#gamma1').style.top = '177px';
        }
        function setTab(n) {
            for (var i = 1; i <= 5; ++i) {
                document.getElementById('#list-' + i).classList.remove('active');
            }
            document.getElementById('#list-' + n).classList.add('active');
            switch(n) {
                case 1:

                    break;
                case 2:

                    break;
                case 3:

                    break;
                case 4:

                    break;
                case 5:

                    break;
            }
        }
        function invertMainBlock() {
            a = document.getElementById('#delta');
            b = document.getElementById('#delta3');
            if (a.style.display == 'none') {
                b.style.display = 'none';
                a.style.display = 'block';
            }
            else {
                a.style.display = 'none';
                b.style.display = 'block';
            }
        }
    </script>
    <style type="text/css">
